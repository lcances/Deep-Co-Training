{
  "general":
  {
    "sampling_rate": 22050,
    "length": 4,
    "audioroot": "../dataset/audio"
  },

  "augments":
  {
    "TimeStretch":
    {
      "ratio": 1.0,
      "rate": [0.9, 1.1]
    },

    "PitchShiftRandom":
    {
      "ratio": 1.0,
      "sampling_rate": 22050,
      "steps": [-3, 3]
    },

    "PitchShiftChoice":
    {
      "ratio": 1.0,
      "sampling_rate": 22050,
      "choice": [-2, -1, 1, 2]
    },

    "Level":
    {
      "ratio": 1.0,
      "rate": [0.8, 1.2]
    },

    "Noise": {
      "ratio": 1.0,
      "noise_factor": [0.1, 0.4]
    },

    "Occlusion": {
      "ratio": 1.0,
      "sampling_rate": 22050,
      "max_size": 1
    }

  }
}